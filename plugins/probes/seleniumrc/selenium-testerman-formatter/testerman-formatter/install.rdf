<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
<Description about="urn:mozilla:install-manifest">
	<em:id>testerman-formatter@anevia.com</em:id>
	<em:name>Selenium IDE: Testerman ATS Formatter</em:name>
	<em:version>1.1</em:version>
	<em:creator>Anevia S.A.</em:creator>
	<em:developer>Rolf Schroeder</em:developer>
	<em:description>Testerman ATS formatter for Selenium IDE</em:description>
