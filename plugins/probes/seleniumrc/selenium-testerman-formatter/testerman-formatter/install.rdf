	</em:targetApplication>

	<em:requires>   
		<Description>
			<em:id>{a6fd85ed-e919-4a43-a5af-8da18bda539f}</em:id> <!-- Selenium IDE -->
			<em:minVersion>1.0.5-SNAPSHOT</em:minVersion>
			<em:maxVersion>1.*</em:maxVersion>
		</Description>  
	</em:requires>
</Description>

